{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "noImplicitAny": false,
    "removeComments": true,
     "experimentalDecorators": true,
    "noLib": false,
    "jsx": "react",
    "outDir": "dist",
    "lib": ["es6","es7","es2015","es2016","es2017","dom"],
    "sourceMap": true
  },
  "exclude": [
    "node_modules",
    "static",
    "dist",
    "cordova",
    "test"
  ],
  "include": [
    "src/**/*"
   ]
}
